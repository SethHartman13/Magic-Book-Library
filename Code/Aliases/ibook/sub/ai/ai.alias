embed
<drac2>


def main() -> str:

    if (uvar_test := get_uvar("ibook_source_list")):
        uvar = load_json(uvar_test)

        uvar.append("Acquisitions Incorporated (ai)")

    else:
        uvar = ['Aquisitions Incorproated (ai)']
    
    set_uvar('ibook_source_list', dump_json(uvar))


main()


</drac2>
-title "{{ctx.author.display_name}} has added Acquisitions Incorporated (ai) to their sourcebook list"
-thumb "https://raw.githubusercontent.com/SethHartman13/Magic-Book-Library/main/Code/Images/ai.png"
-footer "!ibook sub ai | Updated 10/25/23 | ShadowsStride"